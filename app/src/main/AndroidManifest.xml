<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.luke.assistantmanager">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddPlayerActivity"
            android:label="Add Players">
        </activity>
        <activity
            android:name=".EditPlayerActivity"
            android:label="Edit Players">
        </activity>
        <activity
            android:name=".AddFixtureActivity"
            android:label="Add Fixture">
        </activity>
        <activity
            android:name=".EditFixtureActivity"
            android:label="Edit Fixture">
        </activity>

    </application>

</manifest>